{
  "name": "cameraclick",
  "version": "0.0.3",
  "description": "Simple script to capture images from your browser",
  "main": "dist/index.js",
  "files": ["dist"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lhew/cameraclick.git"
  },
  "keywords": [
    "camera",
    "desktop",
    "mobile",
    "browser",
    "snapshot"
  ],
  "author": "Leonardo Almeida",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lhew/cameraclick/issues"
  },
  "homepage": "https://github.com/lhew/cameraclick#readme",
  "scripts": {
    "build": "rm -rf dist && rollup -c rollup.config.js && node-sass --output-style compressed src/cameraclick.scss dist/cameraclick.css ",
    "build:dev": "webpack -p",
    "start": "webpack-dev-server --mode development --hot --inline --progress --compress",
    "test": "./node_modules/.bin/jest -u",
    "eslint": "./node_modules/.bin/eslint **src/**/*.js --fix",
    "preversion": "npm run eslint && npm test",
    "version": "npm run build",
    "postversion": "git push && git push --tags"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "7.2.2",
    "autoprefixer": "^9.5.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-loader": "8.0.5",
    "babel-plugin-named-asset-import": "^0.3.1",
    "css-loader": "1.0.0",
    "eslint": "^5.16.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-loader": "2.1.1",
    "eslint-plugin-import": "^2.17.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-promise": "^4.1.1",
    "eslint-plugin-standard": "^4.0.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^2.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "image-webpack-loader": "^4.6.0",
    "jest": "^24.7.1",
    "jest-resolve": "23.6.0",
    "node-sass": "^4.12.0",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-preset-env": "6.0.6",
    "postcss-safe-parser": "4.0.1",
    "rollup": "^1.15.3",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-filesize": "^2.0.0",
    "rollup-plugin-json": "^3.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-progress": "^0.4.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-terser": "^5.0.0",
    "rollup-plugin-uglify": "^6.0.2",
    "rollup-plugin-visualizer": "^0.8.0",
    "sass-loader": "^7.1.0",
    "style-loader": "0.23.0",
    "webpack": "^4.33.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-server": "^3.7.1"
  }
